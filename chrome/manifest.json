{
  "manifest_version": 2,
  "name": "Desktop streamer for 사다리필름",
  "description": "화면 공유를 위한 jidesha 기반의 크롬 확장 도구입니다",
  "version": "0.1.7",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "logo-16x16.png",
    "48": "logo-48x48.png",
    "128": "logo-128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://consult.wisethinking.co.kr/*"
      ]
  }
}
